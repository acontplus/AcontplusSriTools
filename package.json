{
  "name": "acontplus-sri-tools",
  "version": "1.4.6",
  "description": "Extractor de Comprobantes Electronicos del SRI para Acontplus S.A.S.",
  "private": true,
  "scripts": {
    "dev": "webpack --mode development --watch",
    "build": "webpack --mode production && node scripts/post-build.js",
